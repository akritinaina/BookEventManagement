

@model IEnumerable<Book_Reading_Event_Management.Models.EventDetailsModel>
@using Book_Reading_Event_Management.Models
@{
    ViewBag.Title = "ViewUpcommingEvents";
    var userEmail = ViewBag.useremail;
}

<h2>Upcomming Events</h2>
<h3>@ViewBag.Message</h3>
<ul>
    @foreach (EventDetailsModel eventDetails in @Model)
    {
        <li>@Html.ActionLink(eventDetails.Title, "ViewBookEvents", new { title = eventDetails.Title, email = userEmail })</li>
    }
</ul>





